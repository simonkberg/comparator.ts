{
  "name": "comparator.ts",
  "version": "1.0.1",
  "description": "A TypeScript library for comparing values",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonkberh/comparator.ts.git"
  },
  "license": "MIT",
  "author": "Simon Kjellberg <simon.kjellberg@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:ts && pnpm build:docs",
    "build:docs": "pnpm build:docs:extract && pnpm build:docs:generate && pnpm build:docs:format",
    "build:docs:extract": "api-extractor run",
    "build:docs:format": "prettier --log-level warn -w docs/api/**/*.md",
    "build:docs:generate": "api-documenter markdown -i=docs/api -o=docs/api",
    "build:ts": "tsup",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:eslint": "eslint --max-warnings=0 .",
    "lint:prettier": "prettier --check .",
    "prepare": "husky",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "pretest": "pnpm build",
    "test": "tsx --test test.ts",
    "posttest": "attw --pack ."
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.2",
    "@eslint/js": "9.25.1",
    "@microsoft/api-documenter": "7.26.24",
    "@microsoft/api-extractor": "7.52.5",
    "@tsconfig/recommended": "1.0.8",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.15.2",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
